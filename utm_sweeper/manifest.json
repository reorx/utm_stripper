{
  "manifest_version": 2,
  "name": "UTM Sweeper",
  "short_name": "utm_sweeper",
  "description": "",
  "version": "0.2.1",
  "minimum_chrome_version": "35",

  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "page_action": {
    "default_icon": "images/icon_48.png"
  },
  "permissions": [
    "webNavigation",
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/stripper.min.js",
      "js/background.min.js"
    ]
  }
}
