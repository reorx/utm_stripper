{
  "manifest_version": 2,
  "name": "utm_sweeper",
  "short_name": "utm_sweeper",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "35",

  "icons": {
    "16": "assets/b/icon_16.png",
    "48": "assets/b/icon_48.png",
    "128": "assets/b/icon_128.png"
  },
  "page_action": {
    "default_icon": "assets/b/icon_48.png"
